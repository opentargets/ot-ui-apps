{
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": false,
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "ESNext",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "target": "ESNext",
    "types": ["vite/client"],
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  }
}
